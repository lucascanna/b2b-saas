{
  "preview": {
    "services": {
      "nextjs": {
        "type": "bun",
        "directory": ".",
        "is_entrypoint": true,
        "external_connection_variable": "NEXT_PUBLIC_APP_URL",
        "dbMigrateCommand": "db:migratepippo"
        "develop_command": "bun run dev",
        "deploy_command": "bun run build && bun run start"
      }
    },
    "storages": {
      "postgres": {
        "type": "postgres",
        "connection_variable": "POSTGRES_URL"
      },
      "redis": {
        "type": "redis",
        "connection_variable": "REDIS_URL"
      }
    },
    "environment": {
      "GOOGLE_AI_API_KEY": "replace-me",
      "BETTER_AUTH_SECRET": "mysecretusedbythesandoxenviornment",
      "RESEND_API_KEY": "replace-me",
      "RESEND_FROM_EMAIL": "replace-me",
      "RESEND_FROM_NAME": "replace-me",
      "RESEND_REPLY_TO": "replace-me",
      "STRIPE_SECRET_KEY": "replace-me",
      "STRIPE_PUBLISHABLE_KEY": "replace-me",
      "STRIPE_PRO_PRICE_ID": "replace-me",
      "STRIPE_BUSINESS_PRICE_ID": "replace-me",
      "STRIPE_WEBHOOK_SECRET": "replace-me",
      "STRIPE_SUCCESS_URL": "http://localhost:3000/settings/billing",
      "STRIPE_CANCEL_URL": "http://localhost:3000/settings/billing"
    },
    "test_users": [
      {
        "email": "jane+kosuke_test@example.com",
        "displayName": "Jane Smith",
        "role": "admin"
      },
      {
        "email": "john+kosuke_test@example.com",
        "displayName": "John Doe",
        "role": "admin"
      }
    ]
  },
  "production": {
    "services": {
      "nextjs": {
        "type": "web",
        "runtime": "node",
        "directory": ".",
        "build_command": "bun install && bun run build",
        "start_command": "bun run start",
        "is_entrypoint": true,
        "external_connection_variable": "NEXT_PUBLIC_APP_URL"
      },
      "worker": {
        "type": "worker",
        "runtime": "node",
        "directory": ".",
        "build_command": "bun install",
        "start_command": "bun run workers:start"
      }
    },
    "storages": {
      "postgres": {
        "type": "postgres",
        "connection_variable": "POSTGRES_URL"
      },
      "redis": {
        "type": "keyvalue",
        "connection_variable": "REDIS_URL",
        "maxmemory_policy": "noeviction"
      },
      "s3": {
        "type": "s3",
        "access_key_id_variable": "S3_ACCESS_KEY_ID",
        "secret_access_key_variable": "S3_SECRET_ACCESS_KEY",
        "bucket_variable": "S3_BUCKET",
        "region_variable": "S3_REGION",
        "endpoint_variable": "S3_ENDPOINT"
      }
    },
    "resources": {
      "nextjs": {
        "plan": "starter",
        "_comment": "Available plans: starter (512MB/0.5CPU/$7), standard (2GB/1CPU/$25), pro (4GB/2CPU/$85), pro_plus (8GB/4CPU/$175), pro_max (16GB/4CPU/$225), pro_ultra (32GB/8CPU/$450)"
      },
      "worker": {
        "plan": "starter",
        "_comment": "Available plans: starter (512MB/0.5CPU/$7), standard (2GB/1CPU/$25), pro (4GB/2CPU/$85), pro_plus (8GB/4CPU/$175), pro_max (16GB/4CPU/$225), pro_ultra (32GB/8CPU/$450)"
      },
      "postgres": {
        "plan": "basic_256mb",
        "_comment": "Available plans: basic-256mb ($6), basic-1gb ($19), basic-4gb ($75), pro-4gb ($55), pro-8gb ($110), pro-16gb ($220), pro-32gb ($440), pro-64gb ($880)"
      },
      "redis": {
        "plan": "starter",
        "_comment": "Available plans: starter (256MB/$10), standard (1GB/$32), pro (5GB/$135), pro_plus (10GB/$250), pro_max (20GB/$550), pro_ultra (40GB/$1100)"
      }
    },
    "environment": {
      "GOOGLE_AI_API_KEY": "",
      "BETTER_AUTH_SECRET": "",
      "RESEND_API_KEY": "",
      "RESEND_FROM_EMAIL": "",
      "RESEND_FROM_NAME": "",
      "RESEND_REPLY_TO": "",
      "STRIPE_SECRET_KEY": "",
      "STRIPE_PUBLISHABLE_KEY": "",
      "STRIPE_PRO_PRICE_ID": "",
      "STRIPE_BUSINESS_PRICE_ID": "",
      "STRIPE_WEBHOOK_SECRET": "",
      "STRIPE_SUCCESS_URL": "",
      "STRIPE_CANCEL_URL": ""
    }
  }
}
